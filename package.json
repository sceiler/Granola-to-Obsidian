		"granola",
		"meeting-notes",
		"sync"
	],
	"author": "Yi Min Yang",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.15.21",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.25.5",
		"eslint": "^9.27.0",
		"obsidian": "latest",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"tslib": "^2.8.1"
	}
}
