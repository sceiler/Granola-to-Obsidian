{
	"name": "granola-sync",
	"version": "2.3.0",
	"description": "Sync your Granola AI notes to your Obsidian vault",
	"main": "main.js",
	"engines": {
		"node": ">=24.0.0"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"granola",
		"meeting-notes",
		"sync"
	],
	"author": "Yi Min Yang",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^25.2.1",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2",
		"obsidian": "latest",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"tslib": "^2.8.1"
	}
}
